<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<Frame name="EasyHealFrame" parent="UIParent" hidden="false" enableMouse="true" movable="true">
   <Size x="300" y="150" />
   <TitleRegion setAllPoints="true"/>
   <Anchors>
      <Anchor point="CENTER" />
   </Anchors>
   <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border">
      <BackgroundInsets>
         <AbsInset left="11" right="12" top="12" bottom="11" />
      </BackgroundInsets>
   </Backdrop>
   <Layers>
      <Layer level="ARTWORK">
         <FontString name="$parentTextTitle" inherits="GameFontNormal" text="EasyScript">
            <Anchors>
               <Anchor point="TOPLEFT" relativeTo="EasyHealFrame">
                  <Offset x="15" y="-15" />
               </Anchor>
            </Anchors>
         </FontString>
         <FontString name="$parentTextFPS" inherits="GameFontNormal" text="Target">
            <Anchors>
               <Anchor point="TOPLEFT" relativeTo="EasyHealFrame">
                  <Offset x="15" y="-60" />
               </Anchor>
            </Anchors>
         </FontString>
         <FontString name="$parentTextDelay" inherits="GameFontNormal" text="Health">
            <Anchors>
               <Anchor point="TOPLEFT" relativeTo="EasyHealFrame">
                  <Offset x="15" y="-85" />
               </Anchor>
            </Anchors>
         </FontString>
         <FontString name="$parentTextMoney" inherits="GameFontNormal" text="Mana">
            <Anchors>
               <Anchor point="TOPLEFT" relativeTo="EasyHealFrame">
                  <Offset x="15" y="-110" />
               </Anchor>
            </Anchors>
         </FontString>
      </Layer>
   </Layers>
   <Scripts>
      <OnLoad>
      EasyHealLoad();
      </OnLoad>
   </Scripts>
</Frame>
</Ui>

